{
  "name": "nodejs-api-server",
  "version": "1.0.0",
  "description": "Minimal boilerplate for node.js API server",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --ignore ./folder/  --exec  env-cmd -f .env.development  babel-node src/index.js",
    "start": "env-cmd -f .env.staging  babel-node src/index.js",
    "test": "NODE_ENV=test mocha tests/**/*.test.js --require @babel/register --exit",
    "prod": "NODE_ENV=production  env-cmd -f .env.production  babel-node src/index.js",
    "worker": "babel-node src/worker.js",
    "worker:dev": "nodemon --exec babel-node src/worker.js",
    "nodemon:wk": "nodemon --exec babel-node src/worker.js env-cmd -f .env.development --ignore ./folder/",
    "nodemon": "nodemon --exec babel-node src/index.js env-cmd -f .env.development --ignore ./folder/"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@bull-board/api": "^4.6.2",
    "@bull-board/express": "^4.6.2",
    "@pinata/sdk": "^2.1.0",
    "apollo-server-express": "2.6.7",
    "body-parser": "^1.20.0",
    "bull": "^4.10.1",
    "bull-arena": "^3.30.0",
    "canvas": "^2.10.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "files-from-path": "^0.2.6",
    "fs-extra": "^10.1.0",
    "gif-encoder-2": "^1.0.5",
    "graphql": "14.4.0",
    "http-status": "^1.5.2",
    "lodash": "^4.17.21",
    "mongoose": ">=6.4.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nft.storage": "^7.0.0",
    "nodemon": "^2.0.20",
    "sha1": "^1.1.1",
    "sharp": "^0.31.3",
    "socket.io": "^4.5.3",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "throng": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.6",
    "@babel/register": "^7.18.6",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "mocha": "^10.0.0"
  }
}